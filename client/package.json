{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "build-local-prod": "npm run clean-dist && webpack -p --config=configs/webpack/localProd.js",
    "clean-dist": "rimraf dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build-local-prod && node express.js",
    "start-prod2": "webpack-dev-server --config=configs/webpack/localprod.js",
    "test": "jest --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.110",
    "@types/match-sorter": "^2.2.0",
    "@types/react-table": "^6.7.11",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "match-sorter": "^2.2.3",
    "path-to-regexp": "^2.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-keydown": "^1.9.7",
    "react-popper": "^1.0.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.5",
    "react-stonecutter": "^0.3.9",
    "react-table": "^6.8.6",
    "reactstrap": "^6.4.0",
    "recharts": "^1.0.0-beta.10"
  },
  "postcss": {}
}
